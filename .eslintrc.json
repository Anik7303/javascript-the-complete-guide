{
    "env": {
        "browser": true,
        "es2020": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": 11,
        "sourceType": "module"
    },
    "rules": {
        "keyword-spacing": [
            "error",
            {
                "after": true,
                "before": true
            }
        ],
        "no-undef": "off",
        "quotes": ["error", "single"],
        "semi": "error"
    }
}
